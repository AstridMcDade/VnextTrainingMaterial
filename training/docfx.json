                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_taskManagementStart/**.md",
                    "tWN_taskManagementStart/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_taskManagementStart/**.md",
                    "tWN_taskManagementStart/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],      

        "resource": [
            {
                "files": [
                    "images/**",
                    "**/*.png",
                    "**/*.jpg",
                    "**/*.jpeg",
                    "**/*.gif"
                ]
            }
        ],
        "overwrite": [
            {
                "exclude": [
                    "obj/**",
                    ".vscode"
                ]
            }
        ],
        "globalMetadata": {
            "_appTitle": "Processware Developer Guide",
            "_appFooter": "Copyright ï¿½ FlowCentric (PTY) Ltd. - Processware Developer Guide",
            "_appLogoPath": "media/companyLogo_RGB.png",
            "_appLightLogoPath": "media/companyLogo_RGB.png",
            "_appDarkLogoPath": "media/companyLogo_White.png",
            "_appFaviconPath": "media/favicon.ico",
            "_enableSearch": true,
            "_enableNewTab": true,
            "_disableContribution": false,
            "_siteVersion": "1.0.0"
        },
        "dest": "_site",
        "globalMetadataFiles": [
        ],
        "fileMetadataFiles": [
        ],
        "template": [
            "default",
            "templates/modern"
        ],
        "postProcessors": [
            "ExtractSearchIndex"
        ],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    },
    "markdownEngineProperties": {
        "markdigExtensions": [
            "Abbreviations",
            "Footnotes",
            "Tables",
            "Helpers",
            "Parsers"
        ]
    }
}