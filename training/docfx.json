                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_LogOn/**.md",
                    "tWN_LogOn/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_Personalisation/**.md",
                    "tWN_Personalisation/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_MainMenu/**.md",
                    "tWN_MainMenu/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_Dashboard/**.md",
                    "tWN_Dashboard/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "files": [
                    "tWN_Pending/**.md",
